{
  "extends": [
    "airbnb",
    "airbnb-typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "react/function-component-definition": [
      2, { "namedComponents": "arrow-function" }
    ],
    "arrow-body-style": ["error", "always"]
  }
}